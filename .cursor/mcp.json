{
    "mcpServers": {
        "deeptask": {
            "command": "npx",
            "args": [
                "-y",
                "--package=deeptask",
                "deeptask"
            ],
            "env": {
                "DEEPSEEK_API_KEY": "DEEPSEEK_API_KEY_HERE",
                "SILICONFLOW_API_KEY": "SILICONFLOW_API_KEY_HERE",
                "GROQ_API_KEY": "GROQ_API_KEY_HERE"
            }
        }
    }
}